SHELL = /bin/sh

PYTHON = python

.PHONY: default check clean realclean

default:

check:
	$(PYTHON) toLinearLinear.py
	$(PYTHON) unitbase.py
	$(PYTHON) chargedParticle.py
	$(PYTHON) strings.py

clean:

realclean: clean
